{{#extend "iframe"}}
{{#content "body"}}
<div class="RoundupWrapper">
  <div class="RoundupDocumentation">
    <h1>{{ userProjectName }}</h1>
    {{{documentation}}}
  </div>
  {{#if renderComponentOverview}}
  <h2 class="Roundup-sectionTitle">Overview</h2>
  {{#if components}}
  <div class="RoundupComponents">
    {{#each components}}
    <div class="RoundupComponent">
      <a href="{{this.url}}" class="RoundupComponent-file">
        {{#if folders}}
        <span class="RoundupComponent-fileFolders">
          {{#each folders}}{{this}} / {{/each}}
        </span>
        {{/if}}&nbsp;{{this.name}}
      </a>
      <div class="RoundupComponent-html">
        {{{this.html}}}
      </div>
    </div>
    {{/each}}
    {{else}}
    <div class="RoundupDocumentation">
      <p>No components found. Check out the <a href="http://example.com" target="_blank">documentation</a> if you need
        help!</p>
    </div>
  </div>
  {{/if}}
  {{/if}}
</div>
{{/content}}
{{/extend}}
