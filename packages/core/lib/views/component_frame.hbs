{{#extend "iframe"}}
  {{#content "body"}}
    <div class="RoundupWrapper">
      <div id="RoundupComponent" class="RoundupComponentView">
        {{{html}}}
      </div>
    </div>
    <script>
      window.validations = {
        html: {{htmlValidation}},
        accessibility: {{accessibilityValidation}},
      }
    </script>
  {{/content}}
{{/extend}}
