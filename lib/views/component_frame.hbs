{{#extend "iframe"}}
  {{#content "body"}}
    <div class="HeadmanWrapper">
      <div id="HeadmanComponent" class="HeadmanComponentView">
        {{{html}}}
      </div>
    </div>
    <script>
      window.validations = {
        html: {{htmlValidation}},
        accessibility: {{accessibilityValidation}},
      }
    </script>
  {{/content}}
{{/extend}}
