{{#extend "iframe"}}
{{#content "body"}}
<div class="HeadmanWrapper">
  <div class="HeadmanDocumentation">
    <h1>{{ userProjectName }}</h1>
    {{{documentation}}}
  </div>
  {{#if components}}
  <h2 class="Headman-sectionTitle">Overview</h2>
  <div class="HeadmanComponents">
    {{#each components}}
    <div class="HeadmanComponent">
      <a href="{{this.url}}" class="HeadmanComponent-file">
        {{#if folders}}
        <span class="HeadmanComponent-fileFolders">
          {{#each folders}}{{this}} /{{/each}}
        </span>
        {{/if}}&nbsp;{{this.name}}
      </a>
      <div class="HeadmanComponent-html">
        {{{this.html}}}
      </div>
    </div>
    {{/each}}
    {{else}}
    <div class="HeadmanComponent">
      There are no components yet.
    </div>
  </div>
  {{/if}}
</div>
{{/content}}
{{/extend}}
